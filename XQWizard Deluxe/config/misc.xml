	<!-- Look and Feel Packages -->
	<!-- Metouia is the Default setting -->
	<look-and-feel>
		<name>Metouia</name>
		<class>net.sourceforge.mlf.metouia.MetouiaLookAndFeel</class>
		<class-path>metouia.jar</class-path>
	</look-and-feel>
    <!-- Quaqua is the default on Mac OS X -->
    <look-and-feel>
        <name>Quaqua</name>
        <class>ch.randelshofer.quaqua.QuaquaManager</class>
    </look-and-feel>
